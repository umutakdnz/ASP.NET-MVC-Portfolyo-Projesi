@model List<AcunMedyaPortolfyoProject.Models.Job>

<div class="col-lg-6" data-aos="fade-up" data-aos-delay="200">
    <h3 class="resume-title">Deneyimler</h3>

    @foreach (var item in Model)
    {
        <div class="resume-item">
            <h4>@item.Title</h4>
            <h5>@(item.StartDate != null ? ((DateTime)item.StartDate).ToString("dd.MM.yyyy") : "") - @(item.EndDate != null ? ((DateTime)item.EndDate).ToString("dd.MM.yyyy") : "")</h5>
            <p><em>@item.CompanyName</em></p>
            <ul>
                <li>@item.Description</li>
            </ul>
        </div>
    }
    <!-- Edn Resume Item -->
</div>